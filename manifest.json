{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "assets/icon19.png",
         "38": "assets/icon38.png"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Alt+S",
            "linux": "Alt+S",
            "mac": "Alt+S",
            "windows": "Alt+S"
         }
      }
   },
   "description": "Sound Compressor",
   "icons": {
      "128": "assets/icon128.png"
   },
   "manifest_version": 2,
   "name": "Sound Compressor",
   "permissions": [ "<all_urls>" ],
   "version": "0.1"
}
